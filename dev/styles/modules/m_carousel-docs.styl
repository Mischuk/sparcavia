.m_carousel-docs
  background #f5f5f5
  margin-bottom 25px
  padding 25px 1%
  .slick-prev
    left -1%
  .slick-next
    right -1%
  .image
    display block
    width 86%
    margin-left auto
    margin-right auto
    height 210px
    background-repeat no-repeat
    background-position top center
    background-size cover
    @media screen and (max-width: 768px)
      background-size contain
  .title
    margin-top 10px
    text-align center
    font-size 13px
    a
      underline()
